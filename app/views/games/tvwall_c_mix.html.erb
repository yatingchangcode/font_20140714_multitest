<%
  @space_top = "140"
  @space_left = "5%"
  @space_right = "4%"
  @canvas_scale = 0.171
  @canvas_border = 9
  @font_formula = "w / 3 * 14.5 / 15"
  @name_padding = "6px"
  @bar_spacing = "40"
  @bar_formula = "(w + 5) / 2"
  @timebar_height = "16px"
  @video_position = "189px"
  @split_width = "3%"
  @block_margin = "3px"
  @split_width_up = "3%"
  @split_width_down = "3%"
  @count_padding = "0.2em"
  @count_size = "3em"
  case @visitors.length
  when 1
    @canvas_scale = 0.263
    @canvas_border = 15
    @space_top = "140"
    @space_left = "25%"
    @space_right = "24%"
    @timebar_height = "24px"  
  when 2
    @canvas_scale = 0.243
    @canvas_border = 15
    @space_top = "140"
    @space_left = "25%"
    @space_right = "24%"
    @timebar_height = "22px"
  when 3
    @canvas_scale = 0.217
    @canvas_border = 13
    @space_left = "18%"
    @space_right = "17%"
    @timebar_height = "20px"
  when 4
    @font_formula = "w / 3 * 13.5 / 15"
    @bar_formula = "(w + 4) / 2"
    @canvas_scale = 0.191
    @canvas_border = 11
    @space_left = "10%"
    @space_right = "9%"
    @timebar_height = "18px"
    @video_position = "52px"
  when 6..10
    @canvas_scale = 0.131
    @canvas_border = 8
    @block_margin = "2px"
    @space_top = "100"
    @bar_spacing = "5%"
    @split_width_up = "3%"
    @split_width_down = "3%"
    @space_left = "10%"
    @space_right = "9%"
    @font_formula = "w / 3 * 14 / 15"
    @bar_formula = "(w + 1) / 2"
    @timebar_height = "16px"
    case @visitors.length
    when 6
       @split_width_up = "12%" 
       @split_width_down = "12%" 
       @space_left = "13%"
       @space_right = "12%"
       @video_position = "32px"
    when 7
      @split_width_up = "7.5%" 
      @split_width_down = "12%" 
      @space_left = "11.5%"
      @space_right = "10.5%"
    when 8
      @split_width_up = "7.5%" 
      @split_width_down = "7.5%"
      @space_left = "11.5%"
      @space_right = "10.5%"
    when 9
      @split_width_up = "3%" 
      @split_width_down = "7.5%"
    end
  end
  @grid_border = @canvas_border / 3
  @grid_style = "border-radius:" + (@grid_border * 7 / 5).to_s + "px;box-shadow:0px 0px " + (@grid_border * 3).to_s + "px " + @grid_border.to_s + "px rgba(75,217,255,0.7),inset 0px 0px " + @grid_border.to_s + "px " + (@grid_border * 3 / 5).to_s + "px rgba(75,217,255,0.6);"
  @grid_bor_g_style = "box-shadow:0px 0px " + (@grid_border * 3).to_s + "px " + @grid_border.to_s + "px rgba(255,0,12,0.7),inset 0px 0px " + @grid_border.to_s + "px " + (@grid_border * 3 / 5).to_s + "px rgba(255,0,12,0.6);"
%>

